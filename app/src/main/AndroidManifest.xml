    <activity android:name=".features.bk.ui.report.daily.ReportDailyActivity" />
    <activity android:name=".features.dashboard.ui.inputpoint.InputPointActivity" />
    <activity android:name=".features.dashboard.ui.kelas.detailsiswa.DetailSiswaActivity" />
    <activity android:name=".features.newdashboard.konsultasi.KonsultasiActivity" />
    <activity android:name=".features.dashboard.ui.kelas.KelasFragment" />
    <activity android:name=".features.dashboard.ui.rangking.RangkingSiswaFragment" />
    <activity android:name=".features.dashboard.ui.datainformasi.DataInformasiFragment" />
  </application>

</manifest>